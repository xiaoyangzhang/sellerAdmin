#set($layout = "/layout/layout_non.vm")
#parse("layout/layout-meta.vm")
<ul class="clearfix">
    #foreach($roomMessageVO in $roomList)
        <li>
            <div class="radio-bar clearfix">
                <label class="fl"><input type="radio" name="gp" value="$!{roomMessageVO.roomId}" #if($hotelMessageVO.roomId == $roomMessageVO.roomId) checked #end  > 房型$!{velocityCount}</label>
                <img class="fr" src="$!rs_path_jiuxiu/img/droptip_up.jpg">
            </div>
            <div class="bar-box">
                <ul class="clearfix">
                    <li>
                        <label>房型：</label>
                        <label>$!{roomMessageVO.name}</label>
                    </li>
                    <li>
                        <label>面积：</label>
                        <label>$!{roomMessageVO.area}</label>
                    </li>
                    <li>
                        <label>床型：</label>
                        <label>$!{roomMessageVO.bed}</label>
                    </li>
                    <li>
                        <label>可住：</label>
                        <label>$!{roomMessageVO.people}人</label>
                    </li>
                    <li>
                        <label>网络：</label>
                        <label>$!{roomMessageVO.networkStr}</label>
                    </li>
                    <li>
                        <label>窗户：</label>
                        <label>$!{roomMessageVO.windowStr}</label>
                    </li>
                </ul>
                <div class="img-group">
                        <img src="$!rs_path_jiuxiu/img/temp_1.png">
                        <img src="$!rs_path_jiuxiu/img/temp_1.png">
                        <img src="$!rs_path_jiuxiu/img/temp_1.png">
                </div>
            </div>
        </li>
    #end
</ul>