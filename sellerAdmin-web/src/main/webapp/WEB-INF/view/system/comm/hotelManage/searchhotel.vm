#set($layout = "/layout/layout_non.vm")


<table>
    <thead>
    <tr>
        <th></th>
        <th>酒店名称</th>
        <th>区域</th>
        <th>地址</th>
        <th>电话</th>
        <th>详情</th>
    </tr>
    </thead>
    <tbody>
  #foreach($hotelMessageVO in $hotelMessageVOList)
    <tr>
        <td><input type="radio"></td>
        <td>$!{hotelMessageVO.name}</td>
        <td>$!{hotelMessageVO.area}</td>
        <td>$!{hotelMessageVO.locationText}</td>
        <td>$!{hotelMessageVO.phone}</td>
        <td><a>查看详情</a></td>
    </tr>
  #end
    </tbody>
</table>
#parse("/page_jiuniu.vm")
